			<xs:element name="AllResourcesInEMDomain" type="AllResourcesInEMDomain" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CurtailableLoad" type="CurtailableLoad" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Customer" type="Customer" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CustomerAuthorisation" type="CustomerAuthorisation" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DeliveryType" type="DeliveryType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DemandAggregation" type="DemandAggregation" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Device" type="Device" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DispatchableGenerator" type="DispatchableGenerator" minOccurs="1" maxOccurs="1"/>
			<xs:element name="EiTargetType" type="EiTargetType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ElectricalEnergyStoredAggregation" type="ElectricalEnergyStoredAggregation" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ElectricMeter" type="ElectricMeter" minOccurs="1" maxOccurs="1"/>
