	<xs:element name="UsagePoint" type="UsagePoint"/>
	<xs:complexType name="UsagePoint">
		<xs:annotation>
			<xs:documentation>Logical point on a network at which consumption or production is either physically measured (e.g., metered) or estimated (e.g., unmetered street lights).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FSGIMIdentifiedObject">
				<xs:sequence>
					<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A human readable description of the object. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isVirtual" type="xs:boolean" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is used to indicate that the UsagePoint is virtual (that is, not a real physical measurement). This may be the result of a computation or estimation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="roleFlags" type="RoleFlags" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The set of roles pertinent to this UsagePoint</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="status" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Status of this UsagePoint:
0 - Off
1 - On</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndDeviceAssets" type="EndDeviceAsset" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>All end device assets at this usage point.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EnergyUsageInformation" type="EnergyUsageInformation" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>EnergyUsageInformation collection that this UsagePoint is part of.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PositionPoint" type="PositionPoint" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>PositionPoint describing location of this UsagePoint.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ServiceCategory" type="ServiceCategory" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Service category delivered by this service delivery point.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ServiceDeliveryPoints" type="ServiceDeliveryPoint" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
