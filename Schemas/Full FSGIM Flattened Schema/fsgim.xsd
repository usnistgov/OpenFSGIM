				<xs:annotation>
					<xs:documentation>Neutral to ground</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="phaseC">
				<xs:annotation>
					<xs:documentation>Phase C</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="phaseCtoN">
				<xs:annotation>
					<xs:documentation>Phase C to Neutral</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="phaseCtoAv">
				<xs:annotation>
					<xs:documentation>Phase C current or voltage relative to Phase A voltage</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
