		</xs:annotation>
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="ApplicationSpecificPayloadBaseType" type="ApplicationSpecificPayloadBaseType"/>
	<xs:complexType name="ApplicationSpecificPayloadBaseType" abstract="true">
