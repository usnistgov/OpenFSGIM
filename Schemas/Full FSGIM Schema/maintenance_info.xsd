				<xs:annotation>
					<xs:documentation>Information applies to  the collection hardware class.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="collectionSession">
				<xs:annotation>
					<xs:documentation>Information applies to  the collection session.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dataset">
				<xs:annotation>
					<xs:documentation>Information applies to the attribute class.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dimensionGroup">
