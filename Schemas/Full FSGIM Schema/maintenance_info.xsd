			<xs:enumeration value="featureType">
				<xs:annotation>
					<xs:documentation>Information applies to a feature type.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fieldSession">
				<xs:annotation>
					<xs:documentation>Information applies to a field session.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="model">
				<xs:annotation>
					<xs:documentation>Information applies to a model.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nonGeographicDataset">
